#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;cin>>n;
 vector<int>v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
    }
    int l,r;cin>>l>>r;
    for(int i=l;i<=r;i++){
        cout<<v[i]<<' ';
    }
    cout<<endl;
    while(l<=r){
        cout<<v[r]<<' ';
        r--;
    }
    
    return 0;
}
